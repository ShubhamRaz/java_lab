package learning;
class Node{
	int data;
	Node next;
	
	Node(int data){
		this.data = data;
		this.next = null;
	}

Node head;
class createStack{
	
	public void push(int data) {
		Node newNode = new Node(data);
		if(head == null) {
			head = newNode;
			return;
		}
		newNode.next = head;
	}
	
	public void peek() {
		if(head == null) {
			System.out.println("Empty Stack");
			return;
		}
		
		System.out.println(head.data);
		
	}
	
	public void pop() {
		if(head == null) {
			System.out.println("Empty Underflow");
			return;
		}
		
		System.out.println(head.data);
		head = head.next;
	}
}
public class stack {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}

}
