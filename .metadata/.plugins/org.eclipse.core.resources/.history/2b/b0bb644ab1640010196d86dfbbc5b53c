package learning;
import java.util.Scanner;
public class test {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter Your favourate number: ");
		int num = sc.nextInt();
		sc.close();
		switch(num) {
		case 1: System.out.println("You are Stupid");
		case 2,3 : System.out.println("You are Duffer");
		case 4,5,6 : System.out.println("Non sense");
		case 7 : System.out.println("Thala For a reason");
		
		default : System.out.println("You are greate");
		
		}
	}

}
